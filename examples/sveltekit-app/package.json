{
	"name": "sveltekit-example",
	"private": true,
	"version": "0.0.3",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint .",
		"#clean": "rm -rf .svelte-kit"
	},
	"dependencies": {
		"@lumina-auth/core": "workspace:*",
		"@lumina-auth/framework-sveltekit": "workspace:*",
		"@lumina-auth/provider-google": "workspace:*",
		"@lumina-auth/provider-credentials": "workspace:*"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.4",
		"@sveltejs/adapter-auto": "^3.3.1",
		"@sveltejs/kit": "^2.15.2",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"eslint": "^9.17.0",
		"eslint-plugin-svelte": "^2.46.1",
		"globals": "^15.14.0",
		"svelte": "^5.16.6",
		"svelte-check": "^4.1.1",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.19.1",
		"vite": "^6.0.7"
	}
}